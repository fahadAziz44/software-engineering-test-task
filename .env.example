
# REQUIRED: Database credentials
# Application will fail to start if these are not set
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_NAME=postgres

# OPTIONAL: API Key for authentication
# If not set, API key authentication is disabled (development mode)
# In production, set this to a secure random string
API_KEY=your_secret_api_key_here

# Note: Non-sensitive configuration (host, port etc) are in config.yaml
